#include "core.sscript"

class uint32_t    {
    int x = 10;
    
    public uint32_t()    {
        println("Default ctor");    
    }

    public uint32_t(int temp)    {
        println("Called temp: ", temp); 
        int x = temp;             
    }

    public void printVal(float f)    {
         println(x);
         println(f);       
    }
}


int main(int argc, string[] argv)    {
