#include "core.sscript"
#define CLASS class uint32_t
CLASS {
  double x = randomRange(0, 100);

    public uint32_t()    {
      println("Default ctor");    
    }

    public uint32_t(int temp)    {
        println("Called temp: ", temp);          
    }

   public void printVal()    {
        println(x);    
   }
    
   public string toString()    {
        return "LULW";
    }
}

int main(int argc, string[] argv)    {
  uint32_t i = new uint32_t(9841);    
  string s = "Hello";
  string* ptr = &s;
  *ptr = i;
  println(s);
  println(typeof(ptr));

}

